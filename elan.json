{
    "blacklist": [
        "knex",
        "sequelize",
        "pg",
        "pg-hstore",
        "mysql2",
        "mariadb",
        "sqlite3",
        "tedious"
    ],
    "package": {
        "name": "invoices",
        "productName": "Invoices",
        "description": "Application for managing VAT invoices",
        "author": "D-LUSiON",
        "version": "3.0.1",
        "license": "MIT",
        "keywords": [
            "invoices",
            "vat",
            "tool",
            "app",
            "electron",
            "angular"
        ]
    },
    "angular": {
        "prefix": "inv",
        "routing": true,
        "style": "SCSS",
        "minimal": true,
        "skipTests": true
    },
    "template": {
        "name": "default",
        "templateName": "Default template",
        "language": "js",
        "electronRoot": "electron",
        "ngBuildDir": "www",
        "eBuildDir": ""
    },
    "versions": {
        "main": "3.0.1",
        "electron": "3.0.1",
        "angular": {
            "invoices-core": "3.0.1"
        }
    },
    "resources": {
        "modules": "pack-dirs"
    }
}
