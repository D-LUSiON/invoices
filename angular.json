{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "invoices-plg": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "inlineTemplate": true,
                    "inlineStyle": true,
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:module": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "inv",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/invoices-plg",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/modules"
                        ],
                        "styles": [
                            "src/assets/themify-icons.css",
                            "src/assets/segoe-mdl2-icons.css",
                            "src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "baseHref": "",
                            "outputPath": "build/app"
                        },
                        "dev": {
                            "baseHref": "",
                            "outputPath": "www",
                            "deleteOutputPath": false,
                            "optimization": false,
                            "outputHashing": "media",
                            "sourceMap": true,
                            "extractCss": false,
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": false,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.dev.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "invoices-plg:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "invoices-plg:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "invoices-plg:build"
                    }
                }
            }
        },
        "shared": {
            "projectType": "library",
            "root": "projects/shared",
            "sourceRoot": "projects/shared/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/shared/tsconfig.lib.json",
                        "project": "projects/shared/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/shared/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/shared/src/test.ts",
                        "tsConfig": "projects/shared/tsconfig.spec.json",
                        "karmaConfig": "projects/shared/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/shared/tsconfig.lib.json",
                            "projects/shared/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "invoices": {
            "projectType": "library",
            "root": "projects/invoices",
            "sourceRoot": "projects/invoices/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/invoices/tsconfig.lib.json",
                        "project": "projects/invoices/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/invoices/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/invoices/src/test.ts",
                        "tsConfig": "projects/invoices/tsconfig.spec.json",
                        "karmaConfig": "projects/invoices/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/invoices/tsconfig.lib.json",
                            "projects/invoices/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "providers": {
            "projectType": "library",
            "root": "projects/providers",
            "sourceRoot": "projects/providers/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/providers/tsconfig.lib.json",
                        "project": "projects/providers/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/providers/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/providers/src/test.ts",
                        "tsConfig": "projects/providers/tsconfig.spec.json",
                        "karmaConfig": "projects/providers/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/providers/tsconfig.lib.json",
                            "projects/providers/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "sending": {
            "projectType": "library",
            "root": "projects/sending",
            "sourceRoot": "projects/sending/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/sending/tsconfig.lib.json",
                        "project": "projects/sending/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/sending/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/sending/src/test.ts",
                        "tsConfig": "projects/sending/tsconfig.spec.json",
                        "karmaConfig": "projects/sending/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/sending/tsconfig.lib.json",
                            "projects/sending/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "invoices-plg"
}
