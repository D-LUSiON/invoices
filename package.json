{
    "name": "invoices",
    "version": "3.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build:builder": "tsc -p builders/tsconfig.builders.json",
        "build:all": "npm run build:shared && npm run build:invoices",
        "build:shared": "ng build shared && ng build --project extensions --prod --modulePath=shared#SharedModule --extensionName=shared  --outputPath=./src/assets/extensions",
        "build:invoices": "ng build --project extensions --prod --modulePath=./app/invoices/invoices.module#InvoicesModule --extensionName=invoices --sharedLibs=shared --outputPath=./src/assets/extensions",
        "build:providers": "ng build --project extensions --prod --modulePath=./app/providers/providers.module#ProvidersModule --extensionName=providers --sharedLibs=shared --outputPath=./src/assets/extensions"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.13",
        "@angular/common": "~8.2.13",
        "@angular/compiler": "~8.2.13",
        "@angular/core": "~8.2.13",
        "@angular/forms": "~8.2.13",
        "@angular/platform-browser": "~8.2.13",
        "@angular/platform-browser-dynamic": "~8.2.13",
        "@angular/router": "~8.2.13",
        "@fortawesome/fontawesome-free": "^5.11.2",
        "ngx-electron": "^2.2.0",
        "rxjs": "~6.4.0",
        "systemjs": "^3.1.6",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.18",
        "@angular-devkit/build-ng-packagr": "~0.803.19",
        "@angular/cli": "~8.3.18",
        "@angular/compiler-cli": "~8.2.13",
        "@angular/language-service": "~8.2.13",
        "@types/node": "~8.9.4",
        "electron": "^7.1.2",
        "karma": "^4.4.1",
        "ng-packagr": "^5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": "^0.37.0",
        "tslint": "~5.15.0",
        "typescript": "~3.5.3"
    },
    "productName": "invoices",
    "description": "Invoices application v3.0 (rewriten from scratch)",
    "author": "D-LUSiON",
    "license": "MIT",
    "keywords": [
        "angular",
        "electron",
        "invoices",
        "management",
        "app",
        "application",
        "software",
        "free",
        "mit"
    ]
}
