{
    "name": "electron-angular-quickstart",
    "version": "0.0.1",
    "license": "MIT",
    "main": "electron/main.js",
    "scripts": {
        "ng": "ng",
        "start": "gulp copy_dev_environment && concurrently \"npm run watch\" \"electron .\"",
        "watch": "ng build --watch --aot=false --configuration=dev --base-href= --delete-output-path false --output-path=./html_dev",
        "build": "gulp copy_prod_environment && electron-packager-interactive && gulp copy_dev_environment",
        "build:windows": "electron-packager --platform=win32",
        "build:linux": "electron-packager --platform=linux",
        "build:macos": "electron-packager --platform=darwin",
        "build:electron": "gulp setup_electron_before && electron-packager-interactive && gulp setup_electron_after",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": ">5.2.5",
        "@angular/common": ">5.2.5",
        "@angular/compiler": ">5.2.5",
        "@angular/core": "6.0.0",
        "@angular/forms": ">5.2.5",
        "@angular/http": ">5.2.5",
        "@angular/platform-browser": ">5.2.5",
        "@angular/platform-browser-dynamic": ">5.2.5",
        "@angular/router": ">5.2.5",
        "core-js": "2.5.6",
        "electron-window-state": "4.1.1",
        "ngx-electron": "1.0.4",
        "rxjs": "6.1.0",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.6.0",
        "@angular/cli": "6.0.0",
        "@angular/compiler-cli": ">5.2.5",
        "@angular/language-service": ">5.2.5",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/node": "6.0.101",
        "codelyzer": "4.3.0",
        "concurrently": "3.5.1",
        "electron": "1.8.6",
        "electron-packager": "^10.1.2",
        "electron-packager-interactive": "^0.1.3",
        "electron-reload": "^1.2.2",
        "gulp": "3.9.1",
        "gulp-rename": "1.2.2",
        "gulp-run-command": "0.0.9",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "1.4.2",
        "karma-jasmine": "1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "ts-node": "~3.2.0",
        "tslint": "~5.7.0",
        "typescript": "2.7.2"
    }
}
