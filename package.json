{
    "name": "invoices",
    "version": "3.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/ --clean --sort --format namespaced-json",
        "extract": "npm run extract:en && npm run extract:bg",
        "extract:en": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --sort --format namespaced-json",
        "extract:bg": "ngx-translate-extract --input ./src --output ./src/assets/i18n/bg.json --clean --sort --format namespaced-json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.5",
        "@angular/common": "~8.2.5",
        "@angular/compiler": "~8.2.5",
        "@angular/core": "~8.2.5",
        "@angular/forms": "~8.2.5",
        "@angular/platform-browser": "~8.2.5",
        "@angular/platform-browser-dynamic": "~8.2.5",
        "@angular/router": "~8.2.5",
        "@ant-design/icons-angular": "^8.0.3",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "ng-zorro-antd": "^8.1.2",
        "ngx-electron": "^2.1.1",
        "rxjs": "~6.4.0",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.4",
