{
    "name": "invoices",
    "version": "3.0.0",
    "scripts": {
        "postinstall": "npm run build:builder",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "extract-i18n": "ngx-translate-extract --input ./src --output ./src/assets/i18n/ --clean --sort --format namespaced-json",
        "extract": "npm run extract:en && npm run extract:bg",
        "extract:en": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --sort --format namespaced-json",
        "extract:bg": "ngx-translate-extract --input ./src --output ./src/assets/i18n/bg.json --clean --sort --format namespaced-json",
        "build:builder": "tsc -p builders/tsconfig.builders.json",
        "build:all": "npm run build:shared && npm run build:invoices",
        "build:dev:all": "npm run build:dev:shared && npm run build:dev:invoices",
        "build:dev:invoices": "ng build --project plugins --configuration=develop --modulePath=./app/invoices/invoices.module#InvoicesModule --pluginName=invoices --sharedLibs=shared --outputPath=./src/assets/plugins",
        "build:shared": "ng build shared && ng build --project plugins --prod --modulePath=shared#SharedModule --pluginName=shared  --outputPath=./src/assets/plugins",
        "build:invoices": "ng build --project plugins --prod --modulePath=./app/invoices/invoices.module#InvoicesModule --pluginName=invoices --sharedLibs=shared --outputPath=./src/assets/plugins"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.5",
        "@angular/common": "~8.2.5",
        "@angular/compiler": "~8.2.5",
        "@angular/core": "~8.2.5",
        "@angular/forms": "~8.2.5",
        "@angular/platform-browser": "~8.2.5",
        "@angular/platform-browser-dynamic": "~8.2.5",
        "@angular/router": "~8.2.5",
        "@ant-design/icons-angular": "^8.0.3",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "ng-zorro-antd": "^8.1.2",
        "ngx-electron": "^2.1.1",
        "rxjs": "~6.4.0",
        "systemjs": "^3.1.6",
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.4",
        "@angular-devkit/build-ng-packagr": "~0.803.8",
        "@angular/cli": "~8.3.4",
        "@angular/compiler-cli": "~8.2.5",
        "@angular/language-service": "~8.2.5",
        "@biesbjerg/ngx-translate-extract": "^4.2.0",
        "@types/node": "~8.9.4",
        "electron": "^6.0.12",
        "karma": "^4.3.0",
        "ng-packagr": "^5.4.0",
        "ts-node": "~7.0.0",
        "tsickle": "^0.37.0",
        "tslint": "~5.15.0",
        "typescript": "~3.5.3"
    },
    "productName": "invoices",
    "description": "Invoices application v3.0 (rewriten from scratch)",
    "author": "D-LUSiON",
    "license": "MIT",
    "keywords": [
        "angular",
        "electron",
        "invoices",
        "management",
        "app",
        "application",
        "software",
        "free",
        "mit"
    ]
}
