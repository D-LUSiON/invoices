        "build:electron": "gulp setup_electron_before && electron-packager-interactive && gulp setup_electron_after",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "installer:win": "node installer_scripts/createinstaller-win.js"
